{
  "name": "frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "author": "Ricardo Ordo√±ez",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "pnpm run format:check && pnpm run lint:check",
    "lint:check": "npx eslint --max-warnings 0 --config eslint.config.js './src/**/*'",
    "lint:fix": "npx eslint --config eslint.config.js --fix './src/**/*'",
    "format:check": "npx prettier --check './src/**/*.{js,jsx,ts,tsx,css}'",
    "format:fix": "npx prettier --list-different --write './src/**/*.{js,jsx,ts,tsx,css}'",
    "preview": "vite preview",
    "test": "vitest",
    "prepare": "husky"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-feather": "^2.0.10",
    "react-router": "^7.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "prettier": "^3.4.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.17.0",
    "vite": "^6.0.1",
    "vitest": "^2.1.8"
  }
}
